{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "__sapper__/export" }
    }
  ],
  "routes": [
    {
      "src": "/hello-world/",
      "status": 301,
      "headers": { "Location": "/posts/hello-world/" }
    }
  ]
}
